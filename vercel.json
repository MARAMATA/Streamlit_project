{
    "builds": [
      {
        "src": "miniprojet.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "includeFiles": [
            "Superstore_filtered.csv"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "miniprojet.py"
      }
    ]
  }
  